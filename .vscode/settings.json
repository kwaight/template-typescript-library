	"terminal.integrated.fontFamily": "Monaspace Neon, Monaspace Radon, Monaspace Krypton, Monaspace Argon, Monaspace Xenon, Monaspace Neon Var, Monaspace Radon Var, Monaspace Krypton Var, Monaspace Argon Var, Monaspace Xenon Var, monospace",
	"terminal.integrated.fontLigatures.featureSettings": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
	"terminal.integrated.fontLigatures.enabled": true,
